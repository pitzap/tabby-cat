{
    "name": "Drupal API Search",
    "description": "Add support to the omnibox to search the Drupal API",
    "omnibox": {
	"keyword": "dapi"
    },
    "icons": {
	"16": "icon.png"
    },
    "permissions": [
        "tabs"
    ],
    "background": {
	"scripts": ["background.js"]
    },
    "version": "1.0",
    "minimum_chrome_version": "9",
    "manifest_version": 2
}
